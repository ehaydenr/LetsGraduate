                    <% function reqToClassId(class_data, req){  %>
                    <%   console.log(req.department); %>
                    <%   console.log(req.number); %>
                    <%   for(var i = 0; i < class_data.length; ++i){  %>
                    <%     if(class_data[i].department == req.department && class_data[i].number == req.number){ %>
                    <%       console.log("Returning: " + class_data[i].id); %>
                    <%       return class_data[i].id == undefined ? "" : "" + class_data[i].id; %>
                    <%     }  %>
                    <%   }  %>
                    <%   return 'unknown'; %>
                    <% }  %>


                    <% function notSatisfied(classes,dep,num){  %>
                    <%	if(classes.indexOf(dep+num)>-1){  		%>
                    <%		return false;						%>
                    <%	}										%>
                    <%  return true;				    		%>
                    <%											%>
                    <%}                                 		%>


                    <% function notSatisfiedOf(classes,list){ 			%>
                    <%	for(var i=0; i<list.length; i++){				%>
                    <%		if(classes.indexOf(list[i])>-1){  			%>
                    <%			return false;							%>
                    <%		}											%>
                    <%	}												%>
                    <%  return true;				    				%>
                    <%													%>
                    <%}                                 				%>


